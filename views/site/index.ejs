            --purple-500: #a855f7;
            --purple-400: #c084fc;
            --purple-300: #d8b4fe;
            --purple-200: #e9d5ff;
            --pink-500: #ec4899;
            --pink-400: #f472b6;
            --pink-300: #f9a8d4;
            --cyan-500: #06b6d4;
            --cyan-400: #22d3ee;
            --cyan-300: #67e8f9;
            --slate-900: #0f172a;
            --slate-800: #1e293b;
            --green-400: #4ade80;
            --green-300: #86efac;
            --yellow-400: #facc15;
            --orange-500: #f97316;
            --red-500: #ef4444;
