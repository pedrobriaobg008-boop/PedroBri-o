        .image-banner {
            margin-bottom: 2rem;
            position: relative;
            border-radius: 1rem;
            overflow: hidden;
            height: 16rem;
            border: 1px solid rgba(236, 72, 153, 0.3);
        }

        .image-banner img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

