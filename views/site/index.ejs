            content: '';
            position: absolute;
            inset: 0;
            background: linear-gradient(to top, var(--slate-900), rgba(15, 23, 42, 0.5), transparent);
        }

        /* Table */
        .table-container {
            overflow-x: auto;
            border-radius: 1rem;
            border: 1px solid rgba(99, 102, 241, 0.2);
        }

        table {
            width: 100%;
            border-collapse: collapse;
        }

        thead {
            background: linear-gradient(to bottom right, rgba(30, 41, 59, 0.8), rgba(15, 23, 42, 0.8));
        }

        th {
            padding: 1rem 1.5rem;
            text-align: left;
            color: var(--cyan-300);
            font-weight: 600;
            font-size: 0.875rem;
        }

        tbody {
            background: linear-gradient(to bottom right, rgba(30, 41, 59, 0.4), rgba(15, 23, 42, 0.4));
        }

        tr {
            border-top: 1px solid rgba(99, 102, 241, 0.1);
            transition: background-color 0.2s;
        }

        tr:hover {
            background: rgba(99, 102, 241, 0.05);
        }

        td {
            padding: 1rem 1.5rem;
            font-size: 0.875rem;
        }

        /* Stats Cards (Sess√µes) */
        .stats-cards {
            display: grid;
            gap: 1.5rem;
            margin-bottom: 2rem;
            grid-template-columns: 1fr;
        }

        @media (min-width: 768px) {
            .stats-cards {
                grid-template-columns: repeat(3, 1fr);
            }
        }

        .stat-box {
            padding: 1.5rem;
            border-radius: 1rem;
            border: 1px solid;
        }

        .stat-box-green {
            background: linear-gradient(to bottom right, rgba(74, 222, 128, 0.1), rgba(74, 222, 128, 0.05));
            border-color: rgba(74, 222, 128, 0.3);
        }

        .stat-box-blue {
            background: linear-gradient(to bottom right, rgba(59, 130, 246, 0.1), rgba(59, 130, 246, 0.05));
            border-color: rgba(59, 130, 246, 0.3);
